#!/bin/bash -e

algo=$1

accs=$res/muscle_benchmark/accs/bbs
testdir=$z/int/muscle_benchmark2/${algo}_bbs/
input=$z/int/muscle_benchmark2/bca/bbs.bca
outdir=$z/int/muscle_benchmark2/msta_scores

mkdir -p $outdir
out=$outdir/${algo}

reseek \
  -msta_scores $accs \
  -testdir $testdir \
  -input $input \
  -output $out
