#!/bin/bash -e

bench=$1
algo=$2
acc=$3

mb=/z/int/muscle_benchmark

daliz=$src/pylddt/src/daliz.py

if [ ! -s $zscore ] ; then
	echo Not found zscore=$zscore
	exit 1
fi

testmsa=$mb/output_msas/$bench/$algo/$acc
files=$mb/list_files/$bench/$acc.files

if [ ! -s $testmsa ] ; then
	echo Not found testmsa=$testmsa >> /dev/stderr
	exit 1
fi

if [ ! -s $files ] ; then
	echo Not found files=$files >> /dev/stderr
	exit 1
fi

outdir=$res/muscle_benchmark/zscore
mkdir -p $outdir

cd $mb

$daliz \
  --msa $testmsa \
  --pdbfiles $files \
  > $outdir/$bench.$acc

tail -n1 $outdir/$bench.$acc
