#!/bin/bash -e

bench=$1
algo=$2

outdir=$z/int/muscle_benchmark2/msa2lddt/$bench/$algo
mkdir -p $outdir

if [ x$algo == x ] ; then
	echo $0: missing arg $*
	exit 1
fi

for acc in `cat ../accs/$bench`
do
	echo msa2lddt bench=$bench algo=$algo acc=$acc
	./msa2lddt_bench_algo_acc $bench $algo $acc | tee $outdir/$acc
done
